{
  "name": "shiply-sdk",
  "version": "1.2.9",
  "description": "JavaScript SDK for Shiply - Easy feedback collection widget",
  "main": "dist/shiply.min.js",
  "module": "dist/shiply.esm.min.js",
  "types": "dist/shiply.d.ts",
  "exports": {
    ".": {
      "import": "./dist/shiply.esm.min.js",
      "require": "./dist/shiply.min.js",
      "types": "./dist/shiply.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "npm run build:umd && npm run build:esm && npm run build:types",
    "build:umd": "webpack --mode=production",
    "build:esm": "webpack --config webpack.esm.config.js --mode=production",
    "build:dev": "webpack --mode=development",
    "build:types": "cp src/types/index.d.ts dist/shiply.d.ts",
    "dev": "webpack serve --mode=development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write src/**/*.{js,ts,json,md}",
    "format:check": "prettier --check src/**/*.{js,ts,json,md}",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "release": "node scripts/release.js"
  },
  "keywords": [
    "feedback",
    "widget",
    "sdk",
    "javascript",
    "user-feedback",
    "customer-feedback",
    "analytics"
  ],
  "author": "Shiply Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nkalpakis21/shiply-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/nkalpakis21/shiply-sdk/issues"
  },
  "homepage": "https://github.com/nkalpakis21/shiply-sdk#readme",
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.1.5",
    "babel-jest": "^30.1.2",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
